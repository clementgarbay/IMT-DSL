def fullInput(key, value) {
  fill input[name=key] value
}

def main {
  openBrowser "firefox"
  go "www.google.fr"
  click a[text="SignIn"]
  fullInput("username", "truc")
  fullInput("password", "tructruc")
  click checkbox[text="Rember Me"]
  click button[text="SignIn"]
  assign h1[].value title
  assert title = "Welcome"
}